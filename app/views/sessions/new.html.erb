<h2>ログイン</h2>
<%= form_with url: login_path, local: true do |form|%>
  <div>
    <%= form.label :student_no, "学籍番号" %>
    <%= form.text_field :student_no %>
  </div>
  <div>
    <%= form.label :password, "パスワード" %>
    <%= form.password_field :password %>
  </div>
  <%= form.submit "ログイン" %>
<% end %>